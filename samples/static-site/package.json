{
  "name": "static-site",
  "version": "0.1.0",
  "bin": {
    "static-site": "bin/static-site.js"
  },
  "scripts": {
    "build": "tsc",
    "test:constructor": "jest test/constructor",
    "test:deployment": "jest test/deployment",
    "test:functional": "jest test/functional",
    "deploy": "dotenv-ng --var AWS_SDK_LOAD_CONFIG=1 --- cdk deploy",
    "predeploy": "npm run test:constructor",
    "postdeploy": "npm run test:deployment && npm run test:functional"
  },
  "devDependencies": {
    "@types/jest": "^29.5.1",
    "@types/node": "20.1.0",
    "aws-cdk": "2.79.1",
    "aws-cdk-assert": "^1.1.0",
    "jest": "^29.5.0",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "typescript": "~5.0.4"
  },
  "dependencies": {
    "@aws-sdk/client-cloudformation": "^3.431.0",
    "@aws-sdk/client-cloudfront": "^3.431.0",
    "@aws-sdk/client-s3": "^3.431.0",
    "aws-cdk-lib": "2.80.0",
    "axios": "^1.4.0",
    "constructs": "^10.0.0",
    "dotenv-ng": "^1.1.4",
    "source-map-support": "^0.5.21"
  }
}
